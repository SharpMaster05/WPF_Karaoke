<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Background" Color="#014751"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="#023b3f"/>
    <SolidColorBrush x:Key="DisableBackground" Color="#1e2d2e"/>
    <SolidColorBrush x:Key="MouseOverBackground" Color="#0a292b"/>
    <SolidColorBrush x:Key="MousePressBackground" Color="#00565c"/>

    <SolidColorBrush x:Key="Foreground" Color="#00ffc2"/>
    
    <SolidColorBrush x:Key="MenuBackground" Color="Black" Opacity="0.1"/>

    

</ResourceDictionary>